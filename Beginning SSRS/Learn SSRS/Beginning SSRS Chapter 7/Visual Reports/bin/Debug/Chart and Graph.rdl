  </DataSources>
  <DataSets>
    <DataSet Name="SalesFor2012">
      <Query>
        <DataSourceName>AdventureWorks</DataSourceName>
        <CommandText>SELECT SUM(TotalDue) AS TotalSales, MONTH(OrderDate) AS OrderMonth, T.TerritoryID, T.Name AS TerritoryName 
FROM Sales.SalesOrderHeader AS SOH 
JOIN Sales.SalesTerritory AS T ON T.TerritoryID = SOH.TerritoryID
WHERE OrderDate &gt;= '1/1/2012' AND OrderDate &lt; '12/01/2013'
GROUP BY MONTH(OrderDate), T.TerritoryID, T.Name;</CommandText>
      </Query>
      <Fields>
        <Field Name="TotalSales">
          <DataField>TotalSales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="OrderMonth">
          <DataField>OrderMonth</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TerritoryID">
          <DataField>TerritoryID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="TerritoryName">
          <DataField>TerritoryName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
